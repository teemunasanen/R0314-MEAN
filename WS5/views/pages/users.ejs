<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include ('../partials/head'); %>
  </head>

  <body class="container">
    <header><%- include ('../partials/header'); %></header>
    <main>
      <div class="jumbotron">
        <table>
          <% users.forEach(function(user){ %>
          <tr>
            <td><%= user.name %></td>
            <td><%= user.age %></td>
          </tr>
          <% }) %>
        </table>
      </div>
    </main>

    <footer><%- include ('../partials/footer'); %></footer>
  </body>
</html>
